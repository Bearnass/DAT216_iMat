                  <ImageView fx:id="menuBarSaleImageView" fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true" />
               </graphic>
            </Button>
            <Button fx:id="menuBarFavouriteButton" mnemonicParsing="false" prefHeight="26.0" prefWidth="91.0" text="Mina favoritvaror">
               <graphic>
                  <ImageView fx:id="menuBarFavouriteImageView" fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../imgs/icons8-heart-30.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="menuBarPurchaseButton" mnemonicParsing="false" text="Tidigare Inköp">
               <graphic>
                  <ImageView fx:id="menuBarPurchaseImageView" fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../imgs/icons8-basket-30.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
        </buttons>
      </ButtonBar>
      <ButtonBar layoutX="506.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button fx:id="menuBarServiceButton" mnemonicParsing="false" prefHeight="26.0" text="Kundtjänst">
               <graphic>
                  <ImageView fx:id="menuBarServiceImageView" fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../imgs/icons8-headphone-58.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="menuBarAccountButton" mnemonicParsing="false" text="Logga in">
               <graphic>
                  <ImageView fx:id="menuBarAccountImageView" fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../imgs/icons8-person-64.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
