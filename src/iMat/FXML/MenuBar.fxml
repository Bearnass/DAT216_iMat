<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="40.0" prefWidth="720.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="iMat.FXML.MenuBar">
   <children>
      <ButtonBar prefHeight="40.0" prefWidth="319.0">
        <buttons>
          <Button fx:id="menuBarHomeButton" mnemonicParsing="false" text="Hem">
               <graphic>
                  <ImageView fx:id="menuBarHomeImageView" fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../imgs/icons8-house-50.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="menuBarSaleButton" mnemonicParsing="false" text="Erbjudanden">
